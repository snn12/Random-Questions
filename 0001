#include <bits/stdc++.h>
using namespace std;

//Iki int tipi giris? verib dörd s?tird? onlarin c?mini, f?rqini, hasilini, qism?tini v? böldükd? olan qaligi çixisa verin.

int main() {
  ios_base::sync_with_stdio(0);
  cin.tie(0);
  
  int a,b;
  cin>>a>>b; cout<<endl;
  
  cout<<"a + b = "<<a+b<<endl;
  cout<<"a - b = "<<a-b<<endl;
  cout<<"a * b = "<<a*b<<endl;
  cout<<"a / b = "<<a/b<<endl;
  cout<<"a % b = "<<a%b<<endl;
  
}
