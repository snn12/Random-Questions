#include <bits/stdc++.h>
using namespace std;

int main() {
    ios_base::sync_with_stdio(0);
    cin.tie(0);

    long long n;
    cin >> n;
	long long reversed = 0;
    
    
    if (n % 10 != 0) {
    while (n > 0) {
        reversed = reversed * 10 + n % 10;
        n /= 10;
    }
    cout << reversed << endl;

} else cout<<"last is 0"<<endl;

    return 0;
}
